<div class="page-header">
  <h3>Saladgram #<%= @saladgram.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">

      <dt>User</dt>
      <dd><%= @saladgram.user_id %></dd>


      <dt>Chat</dt>
      <dd><%= @saladgram.chat_id %></dd>


      <dt>Photo url</dt>
      <dd><%= @saladgram.photo_url %></dd>


      <dt>Caption</dt>
      <dd><%= @saladgram.caption %></dd>


    </dl>

    <div class="btn-group btn-group-justified">

      <a href="/saladgrams" class="btn btn-primary">
        Back
      </a>
      <a href="/saladgrams/<%= @saladgram.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_saladgram/<%= @saladgram.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>

    </div>
  </div>
</div>
